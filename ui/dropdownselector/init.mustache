<li class="dropdown {{#if cssClass}}{{cssClass}}{{/if}}" {{#if id}}id="{{id}}"{{/if}}>
  <a href="#" class="dropdown-toggle" data-toggle="dropdown">
	{{#default}}
	<span class="title">{{display_name}}</span>
	{{/default}}
	<b class="caret"></b>
  </a>
  <ul class="dropdown-menu">
	{{#default}}
	<li><a href="#" {{data 'item'}} class="item">{{display_name}}</a></li>
	{{/default}}
	{{#items}}
	<li><a href="#" {{data 'item'}} class="item">{{display_name}}</a></li>	
	{{/items}}
  </ul>
</li>
