<li class="dropdown active" {{#if htmlId}}id="{{htmlId}}"{{/if}}>
  <a href="#" class="dropdown-toggle" data-toggle="dropdown">
	<span class="title">{{selected}}</span>
	<b class="caret"></b>
  </a>
  <ul class="dropdown-menu">
	{{#defaultOption}}
	<li><a href="{{itemUrl}}" {{data 'item'}} class="item">{{display_name}}</a></li>
	{{/defaultOption}}
	{{#items}}
	<li><a href="{{itemUrl}}" {{data 'item'}} class="item">{{display_name}}</a></li>	
	{{/items}}
  </ul>
</li>
