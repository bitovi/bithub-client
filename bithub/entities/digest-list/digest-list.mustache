<div class="category-digest">
	<div class="truncated">
		<ul class="digest-event-info unstyled">
			{{#digestTypes}}
				{{#digestsFor this}}
					<li class="total {{ normalizedType }}ers">
						{{#isSingular}}
							<a href="{{ digestUrl digests.0 type }}">{{v digests.0.props.origin_author_name }}</a>
						{{ else }}
							{{v digests.length }} people
						{{/isSingular}}
						{{ digestAction type }}
						<span class="popup">
							<a href="{{ targetUrl type project }}">{{ targetName type project }}</a>
							{{^isSingular}}
								<div>
									<span class="people-caret"></span>
									{{#digests}}
										<a href="{{ digestUrl this type}}">{{v props.origin_author_name }}</a>
									{{/digests}}
								</div>
							{{/isSingular}}
						</span>
					</li>
				{{/digestsFor}}
			{{/digestTypes}}
		</ul>
	</div>
</div>