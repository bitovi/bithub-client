<% if (can.route.attr('category') === 'bug') { %>
<div style="padding: 10px 0px; border-bottom: 2px solid #ccc; margin-bottom: 8px">
  <% can.each(items, function(item) { %>
  <a href="<%= filterUrl({state: item.value}) %>" <%= (el) -> el.data('value', item.value) %> style="margin-right: 20px; <%= isSelected(item.value) %>"><%= item.display %></a>
  <% }) %>
</div>
<% } %>
