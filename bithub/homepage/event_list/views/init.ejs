<div id="timespan-filter"></div>

<div id="event-list-container" class="span8 left-content">

	<div class="spinner-top">
		<img src="/assets/images/ajax-loader.gif" alt="loading...">
	</div>

	<div id="event-list" class="event-list">
		<% if (partial() === 'latest') { %>
			<%== latestView.render(this) %>
		<% } else { %>
			<%== greatestView.render(this) %>
		<% } %>

		<div class="spinner-bottom">
			<img src="/assets/images/ajax-loader.gif" alt="loading...">
		</div>

		<% if( !canLoad() ) { %>
			<div class="noMoreContent">... that would be all ...</div>
		<% } %>
	</div>  

</div>
