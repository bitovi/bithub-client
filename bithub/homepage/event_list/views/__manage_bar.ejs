<% if( data.user.isAdmin() ) { %>
<!-- ADMIN -->
<div class="manage-bar row">
  <ul class="nav">
	<% list(data.projects, function( project ) { %>
	<li data-tag="<%= project.attr('name') %>" class="tag-action present_remove"><a href="#" ><small><%= project.attr('name') %></small></a></li>
	<% }) %>
	<li class="dropdown">
	  <a class="dropdown-toggle" data-toggle="dropdown" href="#"><small><%= event.attr('category') %></small><b class="caret"></b></a>
	  <ul class="dropdown-menu">
		<% list(data.categories, function( category ) { %>
		<li class="category-action" data-category="<%= category.attr('name') %>"><a href="#"><%= category.attr('name') %></a></li>
		<% }) %>
	  </ul>
	</li>
  </ul>
  <input type="text" class="transfer-ownership typeahead" placeholder="Transfer ownership">
</div>
<!-- /ADMIN -->
<% } %>
