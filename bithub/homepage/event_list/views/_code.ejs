<div class="truncated code-events">
  <% for( var repo in codeEvents ) { %>

  <% for( var author in codeEvents[repo] ) { %>

  <h5 class="author-title"><%= codeEvents[repo][author].authorName %>:</h5>

  <% for( var title in codeEvents[repo][author].push_event ) { %>

  <h5 class="category-title"><%= codeEvents[repo][author].push_event[title][0].attr('title') %></h5>
  <div><!-- break --></div>
  
  <% list(codeEvents[repo][author].push_event[title], function( event ) { %>

  <% list(event.attr('children'), function( commit ) { %>

  <div class="event list-element commit-event" <%= (el) -> el.data('eventObj', commit)  %> >  
	
	<!-- upvotes -->
	<div class="span1 votes">
	  <div class="uparrow"></div>
	  <div class="count">
		<%= commit.attr('upvotes') %>
	  </div>
	  
	  <div class="clearfix"></div>
	</div>

	<!-- content -->
	<div class="span7 content">

	  <div class="row">

		<div class="metadata-container pull-right">
		  <div class="event-metadata pull-right">
			<small><%== formatTs( commit, day.attr('date') ) %></small>
		  </div>
		</div>

		<h5 class="title">
		  <a href="<%= event.attr('url') %>"><%= commit.attr('source_data.sha') && commit.attr('source_data.sha').substring(0, 6) %> <%== commit.attr('title') %></a>
		</h5>

		<div class="clearfix"></div>	  
	  </div>

	</div>

	<div class="clearfix"></div>
  </div>

  <% }) %>

  <% }) %>
  
  <% } %>
  
  <% if( codeEvents[repo][author].pull_request_event.length > 0 ) { %>
  <h5 class="category-title">Pull requests</h5>
  <div><!-- break --></div>
  
  <% list(codeEvents[repo][author].pull_request_event, function( event ) { %>

  <div class="event list-element commit-event" <%= (el) -> el.data('eventObj', event)  %> >  
	
	<!-- upvotes -->
	<div class="span1 votes">
	  <div class="uparrow"></div>
	  <div class="count">
		<%= event.attr('upvotes') %>
	  </div>
	  
	  <div class="clearfix"></div>
	</div>

	<!-- content -->
	<div class="span7 content">

	  <div class="row">

		<div class="metadata-container pull-right">
		  <div class="event-metadata pull-right">
			<small><%== formatTs( event, day.attr('date') ) %></small>
		  </div>
		</div>

		<h5 class="title">
		  <a href="<%= event.attr('url') %>"><%== event.attr('title') %></a>
		</h5>

		<div class="clearfix"></div>	  
	  </div>

	</div>

	<div class="clearfix"></div>
  </div>
  
  <% }) %>
  <% } %>
  
  <% if( codeEvents[repo][author].create_event.length > 0 ) { %>
  <h5 class="category-title">Created a new branch</h5>
  <div><!-- break --></div>
	 
  <% list(codeEvents[repo][author].create_event, function( event ) { %>
  <div class="event list-element commit-event" <%= (el) -> el.data('eventObj', event)  %> >  
	
	<!-- upvotes -->
	<div class="span1 votes">
	  <div class="uparrow"></div>
	  <div class="count">
		<%= event.attr('upvotes') %>
	  </div>
	  
	  <div class="clearfix"></div>
	</div>

	<!-- content -->
	<div class="span7 content">

	  <div class="row">

		<div class="metadata-container pull-right">
		  <div class="event-metadata pull-right">
			<small><%== formatTs( event, day.attr('date') ) %></small>
		  </div>
		</div>

		<h5 class="title">
		  <a><%== event.attr('title') %></a>
		</h5>

		<div class="clearfix"></div>	  
	  </div>

	</div>

	<div class="clearfix"></div>
  </div>
  <% }) %>
  <% } %>

  <div class="status row" style="margin-left: 67px; margin-top: 5px;">
	<div class="metadata-container pull-right">
	  <div class="event-metadata pull-right"><small>via Github</small></div>
	</div>
	<ul class="tag-list nav">
	  <%== renderEventTags( codeEvents[repo][author].eventForTags, data.visibleTags ) %>
	</ul>
  </div>


  
  <% } %>

  <% } %>
</div>
