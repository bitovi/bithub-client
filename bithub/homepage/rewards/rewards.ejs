<div class="reward-list">	<% list(rewards, function( reward, idx ) { %>	<div class="reward well row">		<div class="img-container span1">			<img src="<%= rewardThumb(reward.image) %>"/>		</div>		<div class="desc span4">			Title: <%= reward.title %><br/>			Point minimum: <%= reward.point_minimum %>		</div>		<div class="clearfix"></div>	</div>	<% }) %></div>