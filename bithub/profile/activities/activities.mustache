<!-- ACTIVITIES --><div id="activities" class="span12">  <ul id="activity-list">	{{#loop user.activities reverse=true}}	<li>	  <span class="points">{{calcPoints}}</span>	  <span class="type">[{{type}}]</span>	  <a href="{{eventUrl}}">{{#if title}}{{title}}{{else}}{{comment}}{{/if}}</a>	  <small>({{prettifyTs created_at format="datetime"}})</small>	</li>	{{/loop}}	  </ul>  </div>