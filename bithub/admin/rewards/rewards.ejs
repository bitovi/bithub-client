<table class="table table-stripped">	<thead>		<tr>			<th>Title</th>			<th>Point minumum</th>			<th></th>		</tr>	</thead>	<tbody>		<% list(rewards, function( reward, idx ) { %>		<tr <%= (el) -> el.data('reward', reward) %> >			<td><a href="<%=can.route.url({page:'admin', view:'rewards', action:'edit', id:reward.id})%>"><%= reward.title %></a></td>			<td><%= reward.point_minimum %></td>			<td><img src="<%= reward.image.image.thumb.url %>"/></td>			<td><button class="delete-reward close">&times;</button></td>		</tr>		<% }) %>	</tbody></table><div class="actions pull-left">	<a class="btn" href="<%= can.route.url({page:'admin', view:'rewards', action:'new'}) %>">New reward</a></div><div class="pagination pull-right"><%== can.route.link("Prev page", {page: 'admin', view: 'rewards', action: 'list', offset: prevOffset }, {className:"btn"}) %><%== can.route.link("Next page", {page: 'admin', view: 'rewards', action: 'list', offset: nextOffset }, {className:"btn"}) %></div>