<table class="table table-stripped">	<thead>		<tr>			<th>Name</th>			<th>Email</th>			<th>Roles</th>			<th>Points</th>		</tr>	</thead>	<tbody>	<% can.each(users, function( user, idx ) { %>	<tr>		<td><a href="<%=can.route.url({page:'admin', view:'users', action:'edit', id:user.id})%>"><%= user.name %></a></td>		<td><%= user.email %></td>		<td><%= user.roles.join(",") %></td>		<td><%= user.score %></td>	</tr>	<% }) %>	</tbody></table><%== can.route.link("Prev page", {page: 'admin', view: 'users', action: 'list', offset: prevOffset }, {className:"btn"}) %><%== can.route.link("Next page", {page: 'admin', view: 'users', action: 'list', offset: nextOffset }, {className:"btn"}) %>