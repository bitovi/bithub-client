{{#latestEvents}}
<div class="day">

  <div class="sep-wrapper wrapper" style="height: 42px;">
	<div class="sep blue">
	  <h4 class="flag blue">{{prettifyTs key format="date"}}</h4>
	</div>
  </div>

  {{#if value.digest}}
  <div class="category-digest">
	<div class="truncated">
	  {{#with value}}
	  {{> digestPartial }}
	  {{/with}}
	</div>
  </div>
  {{/if}}
  
  {{#iterCategories value}}
    <div class="category-{{key}}">
	  <h3 class="{{key}}-icon">{{key}}</h3>
	  <div class="truncated">
	    {{#values}}
		{{> eventPartial }}
	    {{/values}}
	  </div>
    </div>
  {{/iterCategories}}

  {{#if value.chat}}
	<div class="feed-header">
	  <h3 class="irc-icon">Chat <a class="pull-right" href="http://webchat.freenode.net/?channels=canjs"><img src="./assets/images/icon-popout.png" alt="popout"></a></h3>
	</div>
	<div class="truncated irc-chat-container">
	  {{#value.chat}}
	  {{> eventPartial }}
	  {{/value/chat}}
	</div>
  {{/if}}
	
</div>
{{/latestEvents}}
