{{#latestEvents}}
<div class="day">

  <div class="sep-wrapper wrapper" style="height: 42px;">
	<div class="sep blue">
	  <h4 class="flag blue">{{prettifyTs date format="date"}}</h4>
	</div>
  </div>

  {{#if digest}}
  <div class="category-digest">
	<div class="truncated">
	  {{> digestPartial }}
	</div>
  </div>
  {{/if}}
  
  {{#iterCategories}}
    <div class="category-{{category}}">
	  <h3 class="{{category}}-icon">{{category}}</h3>
	  <div class="truncated">
	    {{#events}}
		{{> eventPartial }}
	    {{/events}}
	  </div>
    </div>
  {{/iterCategories}}

  {{#if chat}}
	<div class="feed-header">
	  <h3 class="irc-icon">Chat <a class="pull-right" href="http://webchat.freenode.net/?channels=canjs"><img src="/bithub/assets/images/icon-popout.png" alt="popout"></a></h3>
	</div>
	<div class="truncated irc-chat-container">
	  {{#chat}}
	  {{> eventPartial }}
	  {{/chat}}
	</div>
  {{/if}}
	
</div>
{{/latestEvents}}
