<% if (can.route.attr('view') === 'greatest') { %>
<div id="greatestTimespanFilter" style="padding: 10px 0px; border-bottom: 2px solid #ccc; margin-bottom: 8px">
  <a href="<%= filterUrl({timespan: 'day'}) %>" <%= (el) -> el.data('value', 'day') %>>Day</a> | 
  <a href="<%= filterUrl({timespan: 'week'}) %>" <%= (el) -> el.data('value', 'week') %>>Week</a> |
  <a href="<%= filterUrl({timespan: 'month'}) %>" <%= (el) -> el.data('value', 'month') %>>Month</a> |
  <a href="<%= filterUrl({timespan: 'all'}) %>" <%= (el) -> el.data('value', 'all') %>>All time</a>
  <span style="color:#ccc" class="pull-right">timespan filter &mdash; design to be applied</span>
</div>
<% } %>

<div id="events-list">

  <div class="spinner"><img src="/assets/images/ajax-loader.gif" alt="loading..."></div>

  <div class="events-container">

	<% if (partial() === 'latest') { %>
		<%== latestView.render(this) %>
	<% } else { %>
		<%== greatestView.render(this) %>
	<% } %>
	
  <div class="spinnerBottom"><img src="/assets/images/ajax-loader.gif" alt="loading..."></div>

  </div>  

</div>
